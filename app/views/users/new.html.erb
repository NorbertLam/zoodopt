<h1>Sign Up</h1>

<% if flash[:error] %>
  <li><%= flash[:error] %></li>
<% end %>

<%= form_for @user do |f| %>
  <%= f.label :email%>
  <%= f.text_field :email %>
  <br>
  
  <%= f.label :password %>
  <%= f.text_field :password %>
  <br>
  
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>
  <br>
  
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br>
  
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br>
  
  <%= f.label :age %>
  <%= f.text_field :age %>
  <br>
  
  <%= f.submit %>
  <% end %>


